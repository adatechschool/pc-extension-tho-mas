{
    "manifest_version":2,
    
    "name":"VibeDashboard",
    "description":"Remplace la page d'accueil de Vibe par un dashboard, plus utile que l'ancienne page",
    "version":"1.0",
    "permissions":["storage","activeTab","declarativeContent"],

    "content_scripts":[
        {
            "matches":["*://vibe.adatechschool.fr/*"],
            "js":["js/jquery-3.6.0.min.js","js/content.js"]
        }
    ],

    "page_action":{
        "default_icon":"images/icon.png"
    }

}